{
    "name": "trackmage/trackmage-sdk-php",
    "description": "TrackMage PHP SDK",
    "keywords": ["trackmage", "tracking number", "parcel tracking"],
    "homepage": "https://trackmage.com",
    "license": "MIT",
    "authors": [
        {
            "name": "TrackMage",
            "homepage": "https://trackmage.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5|^7",
        "phpstan/phpstan": "^0.12.83",
        "symfony/console": "*",
        "phpstan/phpstan-phpunit": "^0.12.18"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "TrackMage\\Client\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrackMage\\Client\\" : "test/"
        }
    }
}
